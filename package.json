    "gray-matter": "^4.0.3",
    "marked": "^4.2.5",
    "next": "13.1.1",
    "next-mdx-remote": "^4.2.0",
    "next-themes": "^0.2.1",
    "next-sitemap": "^3.1.44",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.7.1",
    "react-infinite-scroll-component": "^6.1.0",
