{
  "name": "statichunt",
  "description": "statichunt is a static themes and resources directory",
  "version": "2.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "yarn json && node lib/parseNewThemesData.js && next build",
    "lint": "next lint",
    "start": "next start",
    "json": "node lib/jsonGenerator.mjs",
    "ss-themes": "yarn json && node lib/screenshotThemes.mjs",
    "ss-resources": "yarn json && node lib/screenshotResources.mjs",
    "update-themes": "yarn json && node lib/updateThemes.js && yarn json",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs"
  },
